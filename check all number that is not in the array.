
num = [1,3,4,8,10,13]

def mynum(num):
    while num[-2] + 1 != num[-1]:
        for x in range(len(num)):
            if num[x+1] - 1 == num[x]:
                pass
            else:
                num.insert(num[x],num[x]+1)
                
                break
    return num  

f = mynum(num)
print(f)

Input [1,2,3,4,10]
